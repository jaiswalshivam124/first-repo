package com.logic;

public class String23 {

	public static void main(String args[]) {
	String str1 = "ABABAABBBABAAABB";
	int countA=0;
	int countB=0;
	int leng = str1.length();
	for(int i=0;i<leng;i++)
	{
		char ab=str1.charAt(i);
		System.out.print(ab);
		if(ab=='A')
		{
			countA=countA+1;
		}
		else
		{
			countA=countB+1;
		}
	}
	System.out.println(countA+" "+countB);
	}
}
